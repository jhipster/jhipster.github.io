{
  "name": "flat-admin-bootstrap-templates",
  "dependencies": {
    "jquery": "latest",
    "bootstrap": "latest",
    "font-awesome": "latest",
    "datatables": "latest",
    "Chart.js": "latest",
    "animate.css": "latest",
    "ace-builds": "~1.2.2",
    "jquery.easing": "~1.3.1",
    "angular": "~1.4.8"
  },
  "private": true,
  "overrides": {
    "jquery": {
      "main": [
        "dist/jquery.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css",
        "fonts/*"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "fonts/*"
      ]
    },
    "animate.css": {
      "main": [
        "animate.min.css"
      ]
    },
    "datatables": {
      "main": [
        "media/css/jquery.dataTables.min.css",
        "media/css/dataTables.bootstrap.css",
        "media/js/jquery.dataTables.min.js",
        "media/js/dataTables.bootstrap.min.js"
      ]
    },
    "ace-builds": {
      "main": [
        "src/ace.js",
        "src/mode-html.js",
        "src/theme-github.js"
      ]
    },
    "Chart.js": {
      "main": [
        "Chart.min.js"
      ]
    },
    "jquery.easing": {
      "main": [
        "js/jquery.easing.min.js"
      ]
    },
    "angular": {
      "main": [
        "angular.min.js"
      ]
    }
  }
}
