{
  "name": "jhipster-public-website",
  "dependencies": {
    "jquery": "~3.2.1",
    "bootstrap": "~3.3.7",
    "font-awesome": "~4.7.0",
    "datatables": "~1.10.15",
    "Chart.js": "~2.5.0",
    "animate.css": "~3.5.2",
    "ace-builds": "~1.2.2",
    "jquery.easing": "~1.3.1",
    "angular": "~1.6.4",
    "angular-markdown-directive": "~0.3.1"
  },
  "private": true,
  "overrides": {
    "jquery": {
      "main": [
        "dist/jquery.min.js"
      ]
    },
    "bootstrap": {
      "main": [
        "dist/js/bootstrap.min.js",
        "dist/css/bootstrap.min.css",
        "fonts/*"
      ]
    },
    "font-awesome": {
      "main": [
        "css/font-awesome.min.css",
        "fonts/*"
      ]
    },
    "animate.css": {
      "main": [
        "animate.min.css"
      ]
    },
    "datatables": {
      "main": [
        "media/css/jquery.dataTables.min.css",
        "media/css/dataTables.bootstrap.css",
        "media/js/jquery.dataTables.min.js",
        "media/js/dataTables.bootstrap.min.js"
      ]
    },
    "ace-builds": {
      "main": [
        "src/ace.js",
        "src/mode-html.js",
        "src/theme-github.js"
      ]
    },
    "Chart.js": {
      "main": [
        "Chart.min.js"
      ]
    },
    "jquery.easing": {
      "main": [
        "js/jquery.easing.min.js"
      ]
    },
    "angular": {
      "main": [
        "angular.min.js"
      ]
    },
    "angular-sanitize": {
      "main": [
        "angular-sanitize.min.js"
      ]
    },
    "showdown": {
      "main": [
        "compressed/Showdown.min.js"
      ]
    },
    "angular-markdown-directive": {
      "main": [
        "markdown.js"
      ]
    }
  },
  "resolutions": {
    "angular": "1.6.4"
  }
}
